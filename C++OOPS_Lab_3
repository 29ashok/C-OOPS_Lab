#include<bits/stdc++.h>
using namespace std;

class Index
{
    private:
    float a,b;
    public:
    void get();
    void show(float x,float y){
        double power;
        a = x;
        b = y;
    }
};

void Index::get(){
    double power = 1;
    int n;
    cout << "Enter index: "; 
    cin >> n;
    for(int i=1; i<=n; i++)
        power = power*i;
    cout << "The power function is: " << power << endl;
    
}

int main()
{
    Index p;
    p.show(2,3);
    p.get();
    return 0;
}
